{
    "name": "chhw/commander",
    "description": "This package gives you to automatically generate service/repository class, which is extended by Laravel original make commands.",
    "keywords": ["laravel", "command", "generate"],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "illuminate/console": ">=5.0",
        "illuminate/support": ">=5.0",
        "illuminate/database": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "CHHW\\Commander\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CHHW\\Commander\\CommanderServiceProvider"
            ]
        }
    }
}